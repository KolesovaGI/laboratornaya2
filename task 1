#include <stdio.h>

long int get_fibonacci_via_cycle (int number);

int main ()
{
  int number;

  printf ("Enter a Fibonacci number: ");

  scanf ("%d", &number);
  printf ("%li", get_fibonacci_via_cycle (number));

  return 0;
}

long int get_fibonacci_via_cycle (int number)
{
  long int num1 = 1;
  long int num2 = 1;
  long int num3;

  if (number <= 2)
  {
    return 1;
  }

  else
  {
    for (int i = 3; i <= number; i++)
    {
      num3 = num1 = num2;
      num1 = num2;
      num2 = num3;
    }
  }

  return num2;
}
 
