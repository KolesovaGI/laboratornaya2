#include <stdio.h>

long get_fibbonachi_via_recursion(long num);

void main()
{
    long num;
    scanf("%ld", &num);
    printf("%ld", get_fibbonachi_via_recursion(num));
}

long get_fibbonachi_via_recursion(long num)
{
    if (num == 0)
    {
        return 0;
    }
    else if (num == 1 || num == 2)
    {
        return 1;
    }

    static long info[] = { 1, 1 };
    if (info[num] == 0)
    {
        info[num] = get_fibbonachi_via_recursion(num - 1) + get_fibbonachi_via_recursion(num - 2);
    }

    return info[num];
}
